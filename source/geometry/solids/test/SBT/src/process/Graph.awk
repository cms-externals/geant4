#!/bin/awk -f
BEGIN {
print "# Graph.fred generated by process errors" ;
print "/vis/open VRML1" ;
print "/vis/draw/current" ;
print "/fred/volume TORUS1" ;
print "/fred/gun G4" ;
}
{ 
print "/gun/position " $9 " " $10 " " $11 ;  
print "/gun/direction " $12 " " $13 " " $14 ;
print "/run/initialize" ;
print "/vis~/create_view/new_graphics_system VRML1" ;
print "/tracking/storeTrajectory 1"
print "/run/beamOn"
print "/fred/pause"
}
